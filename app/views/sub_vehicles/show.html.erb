<%- model_class = SubVehicle -%>
<div class="page-header">
  <h1><%=t '.title', :default => SubVehicle %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:subvehicle_id) %></dt>
      <dd><pre class="prettyprint"><%= @sub_vehicle.subvehicle_id %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              sub_vehicles_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_sub_vehicle_path(@sub_vehicle), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              sub_vehicle_path(@sub_vehicle),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

